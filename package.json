{
  "name": "@mamillastre/capacitor-environment",
  "version": "6.1.1",
  "description": "Capacitor plugin to get JSON based environment specific data",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "bin": {
    "capacitor-environment": "bin/index.js",
    "capenv": "bin/index.js"
  },
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "bin/",
    "dist/",
    "ios/Plugin/",
    "MamillastreCapacitorEnvironment.podspec"
  ],
  "author": "mamillastre",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mamillastre/capacitor-environment.git"
  },
  "bugs": {
    "url": "https://github.com/mamillastre/capacitor-environment/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "environment",
    "configuration",
    "javascript",
    "typescript",
    "android",
    "ios",
    "cross-platform"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\" --plugin=prettier-plugin-java",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api EnvironmentPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "capacitor:copy:after": "node ./bin/index.js copy"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "kleur": "^4.1.5"
  },
  "devDependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/cli": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/docgen": "^0.3.0",
    "@capacitor/ios": "^7.0.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^8.0.2",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "np": "^10.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-java": "^2.6.6",
    "pretty-quick": "^4.0.0",
    "release-it": "^17.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "swiftlint": "^2.0.0",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "@capacitor/core": ">=7.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}